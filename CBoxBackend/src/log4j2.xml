<?xml version="1.0" encoding="UTF-8"?>
<Configuration packages="eu.drseus.cb.backend.Backend">
	<Appenders>
		<Console name="STDOUT" target="SYSTEM_OUT">
			<!-- <PatternLayout pattern="%d %-5p [%t] %C{2} (%F:%L) - %m%n" /> -->
			<PatternLayout pattern="%d %-5p (%F:%L) - %m%n" />
		</Console>
	</Appenders>
	<Loggers>
		<Logger name="eu.drseus.cb.backend" level="debug">
			<AppenderRef ref="STDOUT" />
		</Logger>
		<Root level="debug">
		</Root>
	</Loggers>
</Configuration>